<meta charset="utf-8">
<title><% if(typeof maintenance === 'undefined' ){ %><%- appMeta.title %><% } else { %><%- appMeta.title502 %><% } %></title>
<meta name="description" content="<% if(typeof maintenance === 'undefined' ){ %><%- appMeta.description %><% } else { %><%- appMeta.description502 %><% } %>">
<meta name="author" content="Mango Information Systems">

<meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="X-UA-Compatible" content="IE=EmulateIE9">

<link rel="stylesheet" href="css/normalize.css">
<link rel="stylesheet" href="css/skeleton.min.css">

<meta property="og:title" content="<% if(typeof maintenance === 'undefined' ){ %><%- appMeta.title %><% } else { %><%- appMeta.title502 %><% } %>" />
<meta property="og:type" content="website" />
<meta property="og:description" content="<% if(typeof maintenance === 'undefined' ){ %><%- appMeta.description %><% } else { %><%- appMeta.description502 %><% } %>" />
<meta property="og:url" content="<%- appMeta.url %>" />
<meta property="og:image" content="<%- appMeta.url + appMeta.thumbnail %>" />

<link rel="icon" type="image/png" href="<%- appMeta.favicon %>">

<% if(typeof ga !== 'undefined') { %>
	<script>
	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

	ga('create', '<%- ga %>', 'auto');
	ga('send', 'pageview');
	</script>

<% } %>
